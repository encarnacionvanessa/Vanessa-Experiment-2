#include <iostream>
using namespace std; 

int main()
{
	int number;
	
	cout << “Please Enter the Number here: ”;
              cin >> number;

	cout << “Shown is the multiplication table for the number “ << number << endl;
	
	for (int i=1 ; i<=10 ; i++)
	{
		cout << number << " x " << i << " = " << number*i << endl;
	}
	
	return 0;
}
 
