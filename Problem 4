#include <iostream>
using namespace std;

int main()
{

int a, b, c;
a = 12;
b = 36;
c = 24;

cout << "Enter the first number: ";
cin >> a;

cout << "Enter the second number: ";
cin >> b;

cout << "Enter the third number: ";
cin >> c;

if (a>b && a>c)
{
	cout << "The largest number is " << a;
}	
else if (b>a && b>c)
{
	cout << "The largest number is " << b;
}
else if (c>a && c>b)
{
	cout << "The largest number is " << c << endl;
}

	
	return 0;
}
 
