#include <iostream>
using namespace std;

int main()
{
	
	int rows, columns;
	
	cout << "Number of Rows: ";
	cin >> rows;
	cout << "Number of Columns: ";
	cin >> columns;
	
	for (int i=1; i <= rows; i++)
	{
		cout << " " <<endl;	
		for (int i=1; i <= columns; i++)
		{
			cout << "*";
		}
	}

	return 0;
}
